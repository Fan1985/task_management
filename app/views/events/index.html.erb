<h1>任務列表</h1>
<table>
  <thead>
    <tr>
      <td>名稱</td>
      <td>優先度</td>
      <td>狀態</td>
      <td>開始時間</td>
      <td>結束時間</td>
    </tr>
  </thead>
  <tbody>
    <% @events.each do |event|%>
    <tr>
      <td><%= event.topic%></td>
      <td><%= event.priority%></td>
      <td><%= event.status%></td>
      <td><%= event.start_from%></td>
      <td><%= event.end_at%></td>
    <% end%>
    </tr>
  </tbody>
</table>